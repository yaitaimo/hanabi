{
    "manifest_version": 2,
    "name": "Hanabi",
    "version": "0.1",
    "description": "Share one of members' happy feeling of merge with Team",
    "permissions": [
        "http://*/",
        "https://*/",
        "tabs",
        "background"
    ],
    "browser_action": {
        "default_icon": "icon19.png",
        "default_title": "Hanabi"
    },
    "background": {
        "scripts": [
            "javascripts/jquery-1.12.1.min.js",
            "javascripts/hanabi.js",
            "javascripts/test.js",
            "javascripts/background.js"
        ]
    }
}
